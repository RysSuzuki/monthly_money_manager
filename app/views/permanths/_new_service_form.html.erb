  <div class='center-form border-warning'>
    <%= form_with(model: Service.new, controller: ServicesController, remote: true) do |form| %>
        <div class="form-group row">
            <%= form.label :name, class:'col-sm-2 col-form-label'%>
            <%= form.text_field :name ,class:'form-control col-sm-10', placeholder: "例：Apple"%>
          </div>
          <div class="form-group row">
            <%= form.label :money, class:'col-sm-2 col-form-label'%>
            <%= form.text_field :money ,class:'form-control col-sm-10', placeholder: "例：10000" %>
          </div>
          <%= form.submit "作成", class:'btn btn-primary'%>
    <% end %>
  </div>